<IfModule mod_rewrite.c>
    RewriteEngine On

    # Atur RewriteBase sesuai dengan subdirektori aplikasi Anda
    RewriteBase /app-rt/

    # Arahkan semua permintaan ke index.php jika file atau direktori yang diminta tidak ada
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . index.php [L]
</IfModule>